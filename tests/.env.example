# Test Environment Configuration
# Copy this file to .env and fill in your values

# Test environment (local, staging, production)
TEST_ENV=local

# Base URL for tests
BASE_URL=http://localhost:3000

# Supabase Configuration (for test authentication)
# These are used by the test fixtures to authenticate with Supabase API
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Service Role Key (required for test data seeding)
# Get from: Supabase Dashboard > Settings > API > service_role key
# This key bypasses RLS to insert test jobs and evaluations
SUPABASE_SERVICE_ROLE_KEY=__n8n_BLANK_VALUE_e5362baf-c777-4d57-a609-6eaf1f9e87f6

# Test User Credentials
# Create a dedicated test user in your Supabase auth for E2E testing
# This user will be used for authenticated tests
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=your_secure_test_password
